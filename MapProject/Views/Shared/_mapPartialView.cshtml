@model MapProject.Models.MapViewModel


    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBa-AFwKRkIkxzgye9Jd7YG0enwOIbhils"></script>
    <script src="~/js/site.js"></script>



    <script type="text/javascript">




      window.onload = function () {
        var markers = JSON.parse('@Model.Markers'.replace(/&quot;/g, '"'));

        getMap(markers);

      }

       

    </script>

    <div class="container py-4">
        <div class="card">
            <div id="dvMap" class="card" style="width: 100%; height: 400px">
            </div>
        </div>
    </div>






